section
  h2#function-{{hrefSafe title}}
    a.header-anchor(href="#function-{{hrefSafe title}}") {{title}}

  {{#each signatures}}
  {{> signature .}}
  {{/each}}

  {{> table table}}

  {{#if prose}}
  :markdown
    {{indent 4 (setLinks prose)}}
  {{/if}}

  {{#each examples}}
  {{> example .}}
  {{/each}}

section
  h2#class-{{hrefSafe title}}
    a.header-anchor(href="#class-{{hrefSafe title}}") {{title}}

  {{#if prototype}}
  blockquote
    h3: a
      :markdown
        Extends {{setLinks prototype}}
  {{/if}}

  {{#if properties}}
  h3 Properties
  {{> table properties}}
  {{/if}}

  {{#if prose}}
  :markdown
    {{indent 4 (setLinks prose)}}
  {{/if}}

  {{#if staticMethods}}
  h3 Static Methods
  {{#each staticMethods}}
  {{> method .}}
  {{/each}}
  {{/if}}

  {{#if instanceMethods}}
  h3 Instance Methods
  {{#each staticMethods}}
  {{> method .}}
  {{/each}}
  {{/if}}
